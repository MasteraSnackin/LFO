{
  "name": "lfo-core",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "test": "node --env-file=.env.test --import tsx/esm --test src/__tests__/routing.test.ts src/__tests__/router.test.ts src/__tests__/android.test.ts",
    "test:cov": "c8 --reporter=text --reporter=lcov node --env-file=.env.test --import tsx/esm --test src/__tests__/routing.test.ts src/__tests__/router.test.ts src/__tests__/android.test.ts",
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "express": "^4.19.2",
    "body-parser": "^1.20.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "c8": "^9.1.0",
    "eslint": "^8.57.0",
    "tsx": "^4.7.2",
    "typescript": "^5.4.3"
  }
}
